{
  "id": "discipleship",
  "label": "Discipleship",
  "format": "custom",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "title": {
        "type": "Text",
        "config": {
          "label": "title",
          "placeholder": "Nome do grupo de discipulado"
        }
      },
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "week_day": {
        "type": "Text",
        "config": {
          "label": "week_day",
          "placeholder": "Dia da semana que o GD acontece"
        }
      },
      "schedule": {
        "type": "Text",
        "config": {
          "label": "schedule",
          "placeholder": "Horário que inicia o GD (exemplo 19h)"
        }
      },
      "biweekly": {
        "type": "Boolean",
        "config": {
          "label": "biweekly",
          "default_value": false,
          "placeholder_true": "true",
          "placeholder_false": "false"
        }
      },
      "address": {
        "type": "StructuredText",
        "config": {
          "label": "address",
          "placeholder": "Endereço do local do GD (apenas bairro e cidade)",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em"
        }
      },
      "leader": {
        "type": "Text",
        "config": {
          "label": "leader",
          "placeholder": "Nome do líder do GD"
        }
      }
    }
  }
}
