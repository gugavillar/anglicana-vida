{
  "id": "subscription",
  "label": "Subscription",
  "format": "custom",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "title": {
        "type": "Text",
        "config": {
          "label": "title",
          "placeholder": "Nome do evento"
        }
      },
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "edition": {
        "type": "Number",
        "config": {
          "label": "edition",
          "placeholder": "Edição do evento (apenas o número)"
        }
      },
      "participant_subscription_open_date": {
        "type": "Date",
        "config": {
          "label": "participant_subscription_open_date",
          "placeholder": "Data de abertura das inscrições dos participantes"
        }
      },
      "participant_subscription_close_date": {
        "type": "Date",
        "config": {
          "label": "participant_subscription_close_date",
          "placeholder": "Data de encerramento das inscrições dos participantes"
        }
      },
      "volunteer_subscription_open_date": {
        "type": "Date",
        "config": {
          "label": "volunteer_subscription_open_date",
          "placeholder": "Data de abertura das inscrições dos voluntários"
        }
      },
      "volunteer_subscription_close_date": {
        "type": "Date",
        "config": {
          "label": "volunteer_subscription_close_date",
          "placeholder": "Data de encerramento das inscrições dos voluntários"
        }
      },
      "initial_date": {
        "type": "Date",
        "config": {
          "label": "initial_date",
          "placeholder": "Data que inicia o evento"
        }
      },
      "final_date": {
        "type": "Date",
        "config": {
          "label": "final_date",
          "placeholder": "Data final do evento"
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "description",
          "placeholder": "Descrição do evento",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em"
        }
      }
    }
  }
}
